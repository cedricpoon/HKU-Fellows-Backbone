{
  "name": "hku-fellows-backbone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": ". ./scripts/set-credential.sh && npm run server",
    "nodemon": ". ./scripts/set-credential.sh && npm run server-ndm",
    "test": "eslint .",
    "server": "node ./bin/www",
    "server-ndm": "./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "docker": "npm run build-docker && . ./scripts/set-credential.sh && npm run start-docker && ./scripts/post-docker.sh",
    "build-docker": "docker build . -t hkuf-backbone",
    "start-docker": "docker run -e RDS_HOSTNAME -e RDS_USERNAME -e RDS_PASSWORD -p 8080:80 -d hkuf-backbone",
    "remove-docker": "./scripts/remove-docker.sh",
    "stop-docker": "./scripts/stop-docker.sh"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "follow-redirects": "^1.7.0",
    "http-errors": "~1.6.2",
    "js-yaml": "^3.12.2",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "showdown": "^1.9.0",
    "swagger-ui-express": "^4.0.2",
    "turndown": "^5.0.3"
  },
  "devDependencies": {
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10"
  }
}
